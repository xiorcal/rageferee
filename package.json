{
  "name": "rageferee",
  "version": "1.4.1",
  "description": "managing rage points since 2021",
  "main": "dist/rage.js",
  "types": "dist/rage.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "tsc -p tsconfig.json && node dist/rage.js",
    "freebies": "tsc -p tsconfig.json && node dist/freeGamez.js",
    "watch": "tsc -p tsconfig.json -w",
    "test": "jest",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiorcal/rageferee.git"
  },
  "author": "xiorcal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiorcal/rageferee/issues"
  },
  "homepage": "https://github.com/xiorcal/rageferee#readme",
  "dependencies": {
    "discord.js": "^14.1.2",
    "dotenv": "^8.2.0",
    "node-cron": "^3.0.0",
    "rss-parser": "^3.12.0",
    "stormdb": "^0.4.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/jest": "^28.0.7",
    "@types/node": "^16.x.x",
    "@types/node-cron": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^6.0.0",
    "jest": "^28.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "standard-version": "^9.1.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist/**/"
  ]
}
