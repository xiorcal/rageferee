    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiorcal/rageferee.git"
  },
  "author": "xiorcal",
  "license": "MIT",
